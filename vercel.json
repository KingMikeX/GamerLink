{
  "builds": [
    {
      "src": "Backend/src/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "webapp/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.)",
      "dest": "Backend/src/main.py"
    },
    {
      "src": "/(.)",
      "dest": "webapp/$1"
    }
  ],
  "installCommand": "npm install --prefix webapp && pip install -r /Backend/src/requirements.txt"
}