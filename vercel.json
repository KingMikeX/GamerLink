{
  "builds": [
    {
      "src": "webapp/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "Backend/src/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/Backend/src/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/webapp/$1"
    }
  ],
  "installCommand": "npm install && pip install -r Backend/src/requirements.txt"
}